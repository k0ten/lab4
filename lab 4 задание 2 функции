#include <stdio.h>
#include <conio.h>

int max(int a, int b) {
    int max = a;
        if (b > max) { max = b; }
    return max;
}
void max4(int a, int b, int c, int d) {
    int maximum;
    if (max(a, b) > max(b, c)) { maximum = max(a, b); }
    else maximum = max(b, c);
    if (max(c, d) > maximum) { maximum = max(c, d); }
    printf("Maksimum etich chisel = %d", maximum);
}

int main() {

    max4(13, 500, 80, 100);
}
